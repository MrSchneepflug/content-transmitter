{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "object-literal-sort-keys": false,
      "variable-name": [
        true,
        "ban-keywords",
        "check-format"
      ],
      "interface-name": {
        "options": [
          "never-prefix"
        ]
      }
    },
    "rulesDirectory": [],
    "linterOptions": {
      "exclude": [
        "dist/**/*",
        "node_modules/**/*"
      ]
    }
  }
